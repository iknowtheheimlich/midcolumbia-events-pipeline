import os
import requests

SERPAPI_KEY = os.getenv("SERPAPI_KEY")

params = {
    "engine": "google_events",
    "q": "Events in Tri-Cities WA",
    "location": "Tri-Cities, Washington, United States",
    "hl": "en",
    "gl": "us",
    "api_key": SERPAPI_KEY
}

resp = requests.get("https://serpapi.com/search", params=params)
resp.raise_for_status()
data = resp.json()
